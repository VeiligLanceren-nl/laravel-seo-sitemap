{
    "name": "veiliglanceren/laravel-seo-sitemap",
    "description": "Laravel Sitemap package to optimize your website in search engines",
    "version": "1.3.2",
    "type": "library",
    "license": "MIT",
    "require": {
        "laravel/framework": "^12.4",
        "illuminate/support": "^12.4",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "scrumble-nl/popo": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^10.1",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.1",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "VeiligLanceren\\LaravelSeoSitemap\\": "src/"
        },
        "files": [
            "src/Support/Helpers/sitemap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Niels Hamelink"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VeiligLanceren\\LaravelSeoSitemap\\SitemapServiceProvider"
            ]
        }
    }
}

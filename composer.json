{
    "name": "veiliglanceren/laravel-seo-sitemap",
    "description": "Laravel Sitemap package to optimize your website in search engines",
    "version": "2.1.0",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "scrumble-nl/popo": "^1.0",
        "ext-dom": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "pestphp/pest": "^2.0 || ^3.0",
        "pestphp/pest-plugin-laravel": "^2.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "VeiligLanceren\\LaravelSeoSitemap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Niels Hamelink"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VeiligLanceren\\LaravelSeoSitemap\\SitemapServiceProvider"
            ]
        }
    }
}
